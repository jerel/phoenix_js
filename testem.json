{
  "framework": "qunit",
  "src_files": [
    "lib/browser-polyfill.js",
    "lib/phoenix.js",
    "lib/tests.js",
    "lib/start-tests.js"
  ],
  "parallel": 1,
  "launch_in_ci": [
    "SL_safari_public_current",
    "SL_safari_public_6_0",
    "SL_internet_explorer_public_current",
    "SL_internet_explorer_public_10",
    "SL_internet_explorer_public_9",
    "SL_internet_explorer_public_8",
    "SL_firefox_public_current",
    "SL_firefox_public_24",
    "SL_chrome_public_current",
    "SL_chrome_public_26"
  ],
  "launch_in_dev": [
    "Chrome"
  ],
  "launchers": {
    "SL_safari_public_current": {
      "exe": "node_modules/.bin/saucie",
      "args": [
        "--tunnelDomains",
        "phoenix-server.dev",
        "--vmVersion",
        "dev-varnish",
        "-b",
        "safari",
        "-v",
        "8.1"
      ],
      "protocol": "tap"
    },
    "SL_safari_public_6_0": {
      "exe": "node_modules/.bin/saucie",
      "args": [
        "--tunnelDomains",
        "phoenix-server.dev",
        "--vmVersion",
        "dev-varnish",
        "--platformSL",
        "OS X 10.8",
        "-b",
        "safari",
        "-v",
        "6.0"
      ],
      "protocol": "tap"
    },
    "SL_internet_explorer_public_current": {
